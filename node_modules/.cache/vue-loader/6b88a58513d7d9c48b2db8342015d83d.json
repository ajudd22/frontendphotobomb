 await axios.get(\"/api/photos\");\n       this.photos = this.response.data;\n     } catch (error) {\n       this.error = error.response.data.message;\n     }\n   },\n   close() {\n    this.show = false;\n    },\n    toggleUpload() {\n      this.show = true;\n    },\n    async uploadFinished() {\n     this.show = false;\n     this.getPhotos();\n     this.error = \"\";\n   },\n    async logout() {\n      try {\n        await axios.delete(\"/api/users\");\n        this.$root.$data.user = null;\n      } catch (error) {\n        this.$root.$data.user = null;\n      }\n    },\n  }\n}\n</script>\n<style scoped>\n.menu {\n  display: flex;\n  justify-content: space-between;\n}\n\n.menu h2 {\n  font-size: 14px;\n}\n</style>\n"]}]}